{
  "name": "genai-polyinfer",
  "version": "1.0.0",
  "description": "Provider-agnostic GenAI orchestrator (no provider SDKs) for browser & Node",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsc -w",
    "test": "vitest",
    "test:unit": "vitest run src/__tests__/unit",
    "test:integration": "vitest run src/__tests__/integration",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "genai",
    "orchestrator",
    "llm",
    "polyinfer"
  ],
  "author": "genai-polyinfer",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.13",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "msw": "^2.11.3",
    "node-fetch": "^2.7.0",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "tsup": "^6.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bugs": {
    "url": "https://github.com/genai-polyinfer/genai-polyinfer/issues"
  },
  "homepage": "https://github.com/genai-polyinfer/genai-polyinfer"
}
